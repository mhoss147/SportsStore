@model IEnumerable<String>

@*creating home button*@ 
<a class="btn btn-block btn-secondary"
   asp-action="List"
   asp-controller="Product"
   asp-route-category="category"
   asp-route-productPage="">
   Home
</a>

@foreach (string category in Model)
{
    <a class="btn btn-block btn-secondary"
       asp-action="List"
       asp-controller="Product"
       asp-route-category="@category"
       asp-route-productPage="">
        @category
    </a>
}